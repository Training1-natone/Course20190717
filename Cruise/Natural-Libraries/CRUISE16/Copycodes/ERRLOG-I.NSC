DEFINE WORK FILE 1 'c:\Training\resources\CruiseRpcLog.txt' ATTRIBUTES 'APPEND'  
COMPRESS '(' *TIME ') - ERROR   - Service:'  &1& '(' P-RESPONSE.P-RSPCODE  ')'
 INTO V-WORKREC
